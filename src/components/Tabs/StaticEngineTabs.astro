>
  <Fragment slot="panel.react"><slot name="react" /></Fragment>
  <Fragment slot="panel.svelte"><slot name="svelte" /></Fragment>
  <Fragment slot="panel.vue"><slot name="vue" /></Fragment>
</Tabs>
